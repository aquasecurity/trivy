<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository>testdata/user/repository</localRepository>
    <proxies>
        <proxy>
            <id>user-proxy-http</id>
            <active>true</active>
            <protocol>http</protocol>
            <host>user.proxy.com</host>
            <port>8080</port>
            <username>user-proxy-user</username>
            <password>user-proxy-pass</password>
            <nonProxyHosts>localhost|*.internal.com</nonProxyHosts>
        </proxy>
        <proxy>
            <id>user-proxy-https</id>
            <active>true</active>
            <protocol>https</protocol>
            <host>user.proxy.com</host>
            <port>8443</port>
        </proxy>
       <proxy>
            <id>user-proxy-inactive</id>
            <active>false</active>
            <protocol>http</protocol>
            <host>inactive.proxy.com</host>
            <port>8080</port>
        </proxy>
    </proxies>
</settings>
