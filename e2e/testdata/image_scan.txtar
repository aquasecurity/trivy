# Image scan test
# This test verifies that Trivy can scan images from both local Docker daemon and remote registry
# and that both methods produce equivalent results

# Pre-pull the image to Docker daemon for local testing
exec docker pull alpine:3.19@sha256:c5b1261d6d3e43071626931fc004f70149baeba2c8ec672bd4f27761f8e1ad6b

# Test 1: Remote image scanning
trivy image --image-src remote --cache-dir $WORK/.cache --format table --output remote_result.txt --db-repository mirror.gcr.io/aquasec/trivy-db@${TRIVY_DB_DIGEST} alpine:3.19@sha256:c5b1261d6d3e43071626931fc004f70149baeba2c8ec672bd4f27761f8e1ad6b

# Verify DB download message appears for remote scan
stdout 'Downloading vulnerability DB...'

# Test 2: Local Docker daemon scanning  
trivy image --image-src docker --cache-dir $WORK/.cache --format table --output local_result.txt --db-repository mirror.gcr.io/aquasec/trivy-db@${TRIVY_DB_DIGEST} alpine:3.19@sha256:c5b1261d6d3e43071626931fc004f70149baeba2c8ec672bd4f27761f8e1ad6b

# Test 3: Exit code testing - scan with exit code for vulnerabilities
! trivy image --exit-code 1 --cache-dir $WORK/.cache --format table --db-repository mirror.gcr.io/aquasec/trivy-db@${TRIVY_DB_DIGEST} alpine:3.19@sha256:c5b1261d6d3e43071626931fc004f70149baeba2c8ec672bd4f27761f8e1ad6b

# Verify all scans completed successfully
! stderr 'FATAL'
exists remote_result.txt
exists local_result.txt

# Verify both scans produce equivalent results
cmp remote_result.txt local_result.txt

# Compare with golden file to ensure expected output format
cmp remote_result.txt image_scan_golden.txt

# Verify cache directory was created
exists $WORK/.cache

# Verify no remote download occurred for local scan (image already pulled)
! stderr 'Downloading.*image'

-- image_scan_golden.txt --

